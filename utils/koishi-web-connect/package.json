{
  "name": "koishi-web-connect",
  "description": "connect web backends for koishi",
  "version": "4.2.6",
  "license": "MIT",
  "author": {
    "name": "arily",
    "email": "arily@arily.moe"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ArilyChan/arilychan.git"
  },
  "main": "lib-commonjs/index.js",
  "module": "lib/index.js",
  "files": [
    "README.md",
    "css/",
    "dist/",
    "lib-commonjs/",
    "lib/"
  ],
  "scripts": {
    "test": "npm run eslint && npm run tsc --noEmit && npm run jest",
    "clean": "del-cli lib lib-commonjs dist css coverage && jest --clearCache",
    "build": "npm run clean && npm run build:js && npm run build:commonjs && npm run build:d.ts && npm run build:cleanup",
    "build:cleanup": "del-cli \"lib?(-*)/**/@(__demo__|__snapshots__|?(*.)test.@(ts|tsx|js|jsx))\"",
    "build:commonjs": "cross-env NODE_ENV=production BABEL_MODULES=cjs babel src --out-dir lib-commonjs --extensions .ts,.tsx,.js,.jsx --copy-files --source-maps",
    "build:d.ts": "npm run tsc --emitDeclarationOnly && npm run tsc --emitDeclarationOnly --outDir lib-commonjs",
    "build:js": "cross-env NODE_ENV=production BABEL_MODULES=false babel src --out-dir lib --extensions .ts,.tsx,.js,.jsx --copy-files --source-maps",
    "eslint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --ignore-pattern \"/__demo__/\" --no-error-on-unmatched-pattern",
    "jest": "cross-env NODE_ENV=test jest --passWithNoTests",
    "prepack": "npm run build",
    "reformat": "npm run eslint --fix --quiet",
    "tsc": "tsc"
  },
  "peerDependencies": {
    "koishi": ">=2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/generator": "^7.21.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-typescript": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.45",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^27.5.1",
    "babel-plugin-const-enum": "^1.2.0",
    "cross-env": "^7.0.3",
    "del-cli": "^4.0.1",
    "express": "^4.18.2",
    "jest": "^27.5.1",
    "koishi": "*",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@types/express": "^4.17.17"
  }
}
